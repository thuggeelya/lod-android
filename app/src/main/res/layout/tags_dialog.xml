<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <AutoCompleteTextView
        android:id="@+id/tags_textView1"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/frame"
        android:completionThreshold="1"
        android:gravity="center"
        android:tag="1"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.156"
        tools:ignore="LabelFor" />

    <AutoCompleteTextView
        android:id="@+id/tags_textView2"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/frame"
        android:completionThreshold="1"
        android:gravity="center"
        android:tag="2"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView1"
        app:layout_constraintVertical_bias="0.050000012"
        tools:ignore="LabelFor" />

    <AutoCompleteTextView
        android:id="@+id/tags_textView3"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/frame"
        android:completionThreshold="1"
        android:gravity="center"
        android:tag="3"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView2"
        app:layout_constraintVertical_bias="0.050000012"
        tools:ignore="LabelFor" />

    <AutoCompleteTextView
        android:id="@+id/tags_textView4"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/frame"
        android:completionThreshold="1"
        android:gravity="center"
        android:tag="4"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView3"
        app:layout_constraintVertical_bias="0.050000012"
        tools:ignore="LabelFor" />

    <AutoCompleteTextView
        android:id="@+id/tags_textView5"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/frame"
        android:completionThreshold="1"
        android:gravity="center"
        android:tag="5"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView4"
        app:layout_constraintVertical_bias="0.050000012"
        tools:ignore="LabelFor" />

    <AutoCompleteTextView
        android:id="@+id/tags_textView6"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:background="@drawable/frame"
        android:completionThreshold="1"
        android:gravity="center"
        android:tag="6"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView5"
        app:layout_constraintVertical_bias="0.050000012"
        tools:ignore="LabelFor" />

    <ImageButton
        android:id="@+id/add_tags_textView_button"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:contentDescription="@string/add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tags_textView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView6"
        app:layout_constraintVertical_bias="0.22000003"
        app:srcCompat="@android:drawable/ic_input_add" />

    <Button
        android:id="@+id/confirm_tags_button"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="@string/OK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tags_textView6"
        app:layout_constraintTop_toBottomOf="@+id/tags_textView6"
        app:layout_constraintVertical_bias="0.22000003" />

</androidx.constraintlayout.widget.ConstraintLayout>